<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <SolidColorBrush x:Key="WindowBackgroundBrush" Color="#0F1419"/>
    <Color x:Key="MyErrorColorValue">#DC3545</Color>
    <Color x:Key="WindowShadowColor" A="255" R="32" G="64" B="96"/>
    <SolidColorBrush x:Key="TitleBarBackgroundBrush" Color="#1E2A3A"/>
    <SolidColorBrush x:Key="TitleBarTextBrush" Color="#E1F5FE"/>
    <SolidColorBrush x:Key="CloseButtonForegroundBrush" Color="#81D4FA"/>
    <SolidColorBrush x:Key="CloseButtonHoverBackgroundBrush" Color="#E74C3C"/>
    <SolidColorBrush x:Key="CloseButtonHoverPressedBackgroundBrush" Color="#C50E1F"/>
    <SolidColorBrush x:Key="CloseButtonHoverForegroundBrush" Color="White"/>
    <SolidColorBrush x:Key="LabelTextBrush" Color="#B3E5FC"/>
    <SolidColorBrush x:Key="InputBackgroundBrush" Color="#263238"/>
    <SolidColorBrush x:Key="InputBorderBrush" Color="#455A64"/>
    <SolidColorBrush x:Key="InputFocusedBorderBrush" Color="#2196F3"/>
    <SolidColorBrush x:Key="InputTextBrush" Color="#E1F5FE"/>
    <SolidColorBrush x:Key="InputCaretBrush" Color="#2196F3"/>
    <SolidColorBrush x:Key="ErrorBackgroundBrush" Color="{StaticResource MyErrorColorValue}"/>
    <SolidColorBrush x:Key="FooterTextBrush" Color="#90CAF9"/>
    <SolidColorBrush x:Key="PrimaryButtonBackgroundBrush" Color="#1976D2"/>
    <SolidColorBrush x:Key="PrimaryButtonForegroundBrush" Color="White"/>
    <SolidColorBrush x:Key="PrimaryButtonHoverBackgroundBrush" Color="#1565C0"/>
    <SolidColorBrush x:Key="PrimaryButtonPressedBackgroundBrush" Color="#0D47A1"/>
    <SolidColorBrush x:Key="SuccessBackgroundBrush" Color="#4CAF50"/>
    <SolidColorBrush x:Key="SuccessButtonHoverBackgroundBrush" Color="#45A049"/>
    <SolidColorBrush x:Key="SuccessButtonPressedBackgroundBrush" Color="#3E8E41"/>
    <SolidColorBrush x:Key="DangerBackgroundBrush" Color="{StaticResource MyErrorColorValue}"/>
    <SolidColorBrush x:Key="DangerButtonHoverBackgroundBrush" Color="#C82333"/>
    <SolidColorBrush x:Key="DangerButtonPressedBackgroundBrush" Color="#A51C2A"/>
    <SolidColorBrush x:Key="SecondaryButtonBackgroundBrush" Color="#37474F"/>
    <SolidColorBrush x:Key="SecondaryButtonHoverBackgroundBrush" Color="#455A64"/>
    <SolidColorBrush x:Key="SecondaryButtonPressedBackgroundBrush" Color="#263238"/>
    <SolidColorBrush x:Key="GoldBrush" Color="Gold"/>
    <SolidColorBrush x:Key="SilverBrush" Color="Silver"/>
    <SolidColorBrush x:Key="BronzeBrush" Color="#CD7F32"/>
    <SolidColorBrush x:Key="NotificationTextBrush" Color="White"/>
</ResourceDictionary>